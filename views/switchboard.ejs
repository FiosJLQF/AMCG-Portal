<% layout('layouts/boilerplate') %>
<%- include('partials/splashbar_switchboard') %>

<div class="container pagetitle">
  <div id="pageTitle">Switchboard</div>
  <hr class="pagetitlebreak">
</div>

<!-- Flex Grid for Body -->
<div class="container">

  <!-- 1st Row of Main Body -->
  <div class="row">

    <!-- Left-hand Panel: Main Content Menu -->
    <%- include('partials/menu_main') %>

    <!-- Right-hand Panel: Content -->
    <% if ( selectedAirportID !== '' ) { %>
      <% if ( selectedAISContentType === '801001' ) { %>
        <%- include('partials/ais_giai') %>
      <% } else if ( selectedAISContentType === '801002' ) { %>
        <%- include('partials/ais_gigb') %>
      <% } else if ( selectedAISContentType === '801003' ) { %>
        <%- include('partials/ais_gioa') %>
      <% } else if ( selectedAISContentType === '801004' ) { %>
        <%- include('partials/ais_gilc') %>
      <% } else if ( selectedAISContentType === '801005' ) { %>
        <%- include('partials/ais_inli') %>
      <% } else if ( selectedAISContentType === '801006' ) { %>
        <%- include('partials/ais_infs') %>
      <% } else { %>  <!-- no specific AIS Content requested, so show the "home" page -->
        <%- include('partials/ais_giai') %>
      <% } %>
    <% } else { %>
      <%- include('partials/switchboard_home') %>
    <% } %>

  </div>    <!-- End row 1 DIV     -->

  <!-- 2nd Row of Main Body -->
  <div class="row">
    <div id="switchboardsidemenu_footer" class="bodylayout1col1-datamgmt"></div>
    <div id="datamgmtform_footer" class="bodylayout1col2-datamgmt">
      <!-- <hr class="bodycol2break"> -->
    </div>
  </div>    <!-- End row 2 DIV     -->
  
</div>      <!-- End container DIV -->