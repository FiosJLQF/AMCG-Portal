      </div><br>

      <!-- Comps Effective Date -->
      <div class="data-mgmt-row">
          <div class="col-3 data-mgmt-sublabel-col">
            <label for="compsEffectiveDate" class="data-mgmt">Comps Effective Date:</label>
          </div>
          <div class="col-1 data-mgmt-required-col">
            <label for="compsEffectiveDate" class="data-mgmt-required"></label>
          </div>
          <div class="col-8 data-mgmt-value-col">
            <input type="date" id="compsEffectiveDate" name="compsEffectiveDate" class="data-mgmt-date"><br>
            <span class="data-mgmt-error" id="compsEffDateErrors"></span>
          </div>
      </div><br>

      <!-- Comparables -->
      <div class="data-mgmt-row">
        <div class="col-3 data-mgmt-sublabel-col">
