          <select id="longestRunwayCategoryHx" name="longestRunwayCategoryHx" class="data-mgmt-hx-text-half" multiple></select>
          <span class="data-mgmt-error" id="runwayCategoryErrors"></span>
        </div>
      </div><br>

      <!-- System Airport -->
      <div class="data-mgmt-row">
        <div class="col-3 data-mgmt-sublabel-col">
          <label for="systemAirport" class="data-mgmt">System Airport:</label>
        </div>
        <div class="col-1 data-mgmt-required-col">
