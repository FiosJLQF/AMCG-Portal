        </div>
        <div class="col-8 data-mgmt-value-col">
          <input type="text" id="competitives" name="competitives" class="data-mgmt-text-half"><br>
          <span class="data-mgmt-error" id="competitivesErrors"></span>
        </div>
      </div><br>

      <!-- Airport Service Level -->
      <div class="data-mgmt-row">
        <div class="col-3 data-mgmt-sublabel-col">
          <label for="airportServiceLevel" class="data-mgmt">Airport Service Level (NPIAS):</label>
        </div>
        <div class="col-1 data-mgmt-required-col">
          <label for="airportServiceLevel" class="data-mgmt-required"></label>
        </div>
        <div class="col-8 data-mgmt-value-col">
          <input type="text" id="airportServiceLevel" name="airportServiceLevel" class="data-mgmt-text-half">
          <button type="button" class="btn btn-default button-normal"
          onclick="toggleBlockShowHide('', document.querySelector('#airportServiceLevelDescHx'),
                       '', '', '', '')">Ext. Hx</button><br>
          <select id="airportServiceLevelDescHx" name="airportServiceLevelDescHx" class="data-mgmt-hx-text-half" multiple></select>
          <span class="data-mgmt-error" id="airportServiceLevelErrors"></span>
        </div>
      </div><br>

      <!-- Hub Classification -->
      <div class="data-mgmt-row">
        <div class="col-3 data-mgmt-sublabel-col">
          <label for="hubClassification" class="data-mgmt">Hub Classification (NPIAS):</label>
        </div>
        <div class="col-1 data-mgmt-required-col">
          <label for="hubClassification" class="data-mgmt-required"></label>
        </div>
        <div class="col-8 data-mgmt-value-col">
          <input type="text" id="hubClassification" name="hubClassification" class="data-mgmt-text-half">
          <button type="button" class="btn btn-default button-normal"
          onclick="toggleBlockShowHide('', document.querySelector('#hubClassificationDescHx'),
                       '', '', '', '')">Ext. Hx</button><br>
          <select id="hubClassificationDescHx" name="hubClassificationDescHx" class="data-mgmt-hx-text-half" multiple></select>
          <span class="data-mgmt-error" id="hubClassificationErrors"></span>
