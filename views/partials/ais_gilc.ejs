    </form>
    <div><br>
      <% if ( selectedAirportID !== '' && userCanUpdateAirport ) { %>
        <button class="data-mgmt" form="airportGILCData" onclick="return validateData();">Save Changes</button>&nbsp;&nbsp;&nbsp;&nbsp;
      <% } %>
    </div><br><br>
    <hr class="bodycol2break">

<script>

  //////////////////////////////////////////////////////////////////////
  // Configure the page
  //////////////////////////////////////////////////////////////////////
  // Set the browser tab name (i.e., title)
  document.title = "AMCG - AIS";
  // Set the page title
  document.getElementById("pageTitle").innerText = "Switchboard - Location/Classification Information";
  // Set the instructions
  document.getElementById('airportInstructions').innerText = "Edit the entries below, and click 'Save Changes'.";

  // Load the Hx SELECT objects
  var latitudeHxRows = <%- JSON.stringify(lfLatitudeHx.rows) %>;
    loadSelectOptionsList("latitudeHx", "", 0, latitudeHxRows);
    document.getElementById('latitudeHx').disabled = true;
    toggleBlockShowHide('', document.querySelector('#latitudeHx'), '', 'hide', '', '');
  var longitudeHxRows = <%- JSON.stringify(lfLongitudeHx.rows) %>;
    loadSelectOptionsList("longitudeHx", "", 0, longitudeHxRows);
    document.getElementById('longitudeHx').disabled = true;
    toggleBlockShowHide('', document.querySelector('#longitudeHx'), '', 'hide', '', '');
  var elevationHxRows = <%- JSON.stringify(lfElevationHx.rows) %>;
    loadSelectOptionsList("elevationHx", "", 0, elevationHxRows);
    document.getElementById('elevationHx').disabled = true;
    toggleBlockShowHide('', document.querySelector('#elevationHx'), '', 'hide', '', '');
  var sectionalChartHxRows = <%- JSON.stringify(lfAeroSectionalChartHx.rows) %>;
    loadSelectOptionsList("sectionalChartHx", "", 0, sectionalChartHxRows);
    document.getElementById('sectionalChartHx').disabled = true;
    toggleBlockShowHide('', document.querySelector('#sectionalChartHx'), '', 'hide', '', '');
  var faaRegionHxRows = <%- JSON.stringify(lfFAARegionCodeDescHx.rows) %>;
    loadSelectOptionsList("faaRegionHx", "", 0, faaRegionHxRows);
    document.getElementById('faaRegionHx').disabled = true;
    toggleBlockShowHide('', document.querySelector('#faaRegionHx'), '', 'hide', '', '');
  var faaDistrictOfficeHxRows = <%- JSON.stringify(lfFAADistrictCodeHx.rows) %>;
    loadSelectOptionsList("faaDistrictOfficeHx", "", 0, faaDistrictOfficeHxRows);
    document.getElementById('faaDistrictOfficeHx').disabled = true;
    toggleBlockShowHide('', document.querySelector('#faaDistrictOfficeHx'), '', 'hide', '', '');
  var ownershipTypeDescHxRows = <%- JSON.stringify(lfOwnershipTypeDescHx.rows) %>;
    loadSelectOptionsList("ownershipTypeDescHx", "", 0, ownershipTypeDescHxRows);
    document.getElementById('ownershipTypeDescHx').disabled = true;
    toggleBlockShowHide('', document.querySelector('#ownershipTypeDescHx'), '', 'hide', '', '');
  var facilityUseDescHxRows = <%- JSON.stringify(lfFacilityUseDescHx.rows) %>;
    loadSelectOptionsList("facilityUseDescHx", "", 0, facilityUseDescHxRows);
    document.getElementById('facilityUseDescHx').disabled = true;
    toggleBlockShowHide('', document.querySelector('#facilityUseDescHx'), '', 'hide', '', '');
  var longestRunwayCategoryHxRows = <%- JSON.stringify(lfLongestRunwayCategoryHx.rows) %>;
    loadSelectOptionsList("longestRunwayCategoryHx", "", 0, longestRunwayCategoryHxRows);
    document.getElementById('longestRunwayCategoryHx').disabled = true;
    toggleBlockShowHide('', document.querySelector('#longestRunwayCategoryHx'), '', 'hide', '', '');
  var airportServiceLevelDescHxRows = <%- JSON.stringify(lfAirportServiceLevelDescHx.rows) %>;
    loadSelectOptionsList("airportServiceLevelDescHx", "", 0, airportServiceLevelDescHxRows);
    document.getElementById('airportServiceLevelDescHx').disabled = true;
    toggleBlockShowHide('', document.querySelector('#airportServiceLevelDescHx'), '', 'hide', '', '');
  var hubClassificationDescHxRows = <%- JSON.stringify(lfHubClassificationDescHx.rows) %>;
    loadSelectOptionsList("hubClassificationDescHx", "", 0, hubClassificationDescHxRows);
    document.getElementById('hubClassificationDescHx').disabled = true;
    toggleBlockShowHide('', document.querySelector('#hubClassificationDescHx'), '', 'hide', '', '');
  var gaCategoryHxRows = <%- JSON.stringify(lfGACategoryHx.rows) %>;
    loadSelectOptionsList("gaCategoryHx", "", 0, gaCategoryHxRows);
    document.getElementById('gaCategoryHx').disabled = true;
    toggleBlockShowHide('', document.querySelector('#gaCategoryHx'), '', 'hide', '', '');

  // Is an aiport being edited?
  <% if ( actionRequested === 'editairport' ) { %>  // "Edit Airport"
    // Set the current values
    document.getElementById('airportIDToUpdate').value = "<%- selectedAirport.LFLocationID_FAA %>";
    document.getElementById('latitude').value = "<%- selectedAirport.LFLatitude %>";
    document.getElementById('latitude').disabled = true;
    document.getElementById('longitude').value = "<%- selectedAirport.LFLongitude %>";
    document.getElementById('longitude').disabled = true;
    document.getElementById('elevation').value = "<%- selectedAirport.LFElevation %>";
    document.getElementById('elevation').disabled = true;
    document.getElementById('sectionalChart').value = "<%- selectedAirport.LFAeroSectionalChart %>";
    document.getElementById('sectionalChart').disabled = true;
    document.getElementById('faaRegion').value = "<%- selectedAirport.LFFAARegionCodeDesc %>";
    document.getElementById('faaRegion').disabled = true;
    document.getElementById('faaDistrictOffice').value = "<%- selectedAirport.LFFAADistrictCode %>";
    document.getElementById('faaDistrictOffice').disabled = true;
    document.getElementById('locationNotes').value = "<%- selectedAirport.LFLocationNotes %>";
    document.getElementById('ownershipType').value = "<%- selectedAirport.LFOwnershipTypeDesc %>";
    document.getElementById('ownershipType').disabled = true;
    document.getElementById('facilityUse').value = "<%- selectedAirport.LFFacilityUseDesc %>";
    document.getElementById('facilityUse').disabled = true;
    document.getElementById('runwayCategory').value = "<%- selectedAirport.LFLongestRunwayCategory %>";
    document.getElementById('runwayCategory').disabled = true;
    document.getElementById('systemAirport').value = "<%- selectedAirport.LFSystemAirport %>";
    document.getElementById('systemAirportIDs').value = "<%- selectedAirport.LFSystemAirportIDs %>";
    document.getElementById('compsEffectiveDate').value = "<%- selectedAirport.LFCompsEffectiveDate %>";
    document.getElementById('comparables').value = "<%- selectedAirport.LFComparablesIDs %>";
    document.getElementById('competitives').value = "<%- selectedAirport.LFCompetitivesIDs %>";
    document.getElementById('airportServiceLevel').value = "<%- selectedAirport.NPIASServiceLevelDesc %>";
    document.getElementById('airportServiceLevel').disabled = true;
    document.getElementById('hubClassification').value = "<%- selectedAirport.NPIASHubClassificationDesc %>";
    document.getElementById('hubClassification').disabled = true;
    document.getElementById('gaCategory').value = "<%- selectedAirport.NPIASGACategory %>";
    document.getElementById('gaCategory').disabled = true;
    document.getElementById('classificationNotes').value = "<%- selectedAirport.LFClassificationNotes %>";

    // Disable the controls (and change the instruction) if the user does not have permission to update AIS data.
    if ( <%- !userCanUpdateAirport %> ) {
      document.getElementById('airportInstructions').innerText = "Review the entries below.";
      document.getElementById('locationNotes').disabled = true;
      document.getElementById('systemAirport').disabled = true;
      document.getElementById('systemAirportIDs').disabled = true;
      document.getElementById('compsEffectiveDate').disabled = true;
      document.getElementById('comparables').disabled = true;
      document.getElementById('competitives').disabled = true;
      document.getElementById('classificationNotes').disabled = true;
    };
  <% } %>


  //////////////////////////////////////////////////////////////////////
  // Validate the data before submitting the form
  //////////////////////////////////////////////////////////////////////
  function validateData() {
    var dataAreValid = true;  // test for errors below

    //////////////////////////////////////////////////
    // If no errors, submit the changes
    //////////////////////////////////////////////////
    return dataAreValid;

  };

</script>