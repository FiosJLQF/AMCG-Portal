        </div>
        <div class="col-1 data-mgmt-required-col">
          <label for="gaCategory" class="data-mgmt-required"></label>
        </div>
        <div class="col-8 data-mgmt-value-col">
          <input type="text" id="gaCategory" name="gaCategory" class="data-mgmt-text-half">
          <button type="button" class="btn btn-default button-normal"
          onclick="toggleBlockShowHide('', document.querySelector('#gaCategoryHx'),
                       '', '', '', '')">Ext. Hx</button><br>
          <select id="gaCategoryHx" name="gaCategoryHx" class="data-mgmt-hx-text-half" multiple></select>
